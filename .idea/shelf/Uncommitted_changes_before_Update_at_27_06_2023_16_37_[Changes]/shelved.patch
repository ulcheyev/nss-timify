+                            <p></p>
+                            <input type="password"
+                                   className="password ele"
+                                   placeholder="Confirm password"
+                                   required/>
+                            <p></p>
+                            <button className="clkbtn">Signup</button>
+                        </form>
+                    </div>
+                </div>
+
+            ))}
+        </>
+        )
+
+}
